@{
    ViewBag.Title = "TrangChuTinTin";
    Layout = "~/Views/Layout/Layout.cshtml";
}
<!--Menu-->
@Html.Action("MenuTinTuc", "HienThi")
<!--//Menu-->
<!--Thanh công cụ-->
@Html.Action("ThanhCongCu", "HienThi")
<!--//Thanh công cụ-->
<!--Giao diện trang chủ-->
<div class="content_middle">
    <div class="container">
        <div class="content_middle_box">
            <center>@Html.Action("DanhSachTheLoai", "TinTuc")</center>
            <div class="bottom_grid wow bounce" data-wow-delay="0.4s">
                @Html.Action("TinTucXemNhieu", "TinTuc")
                @Html.Action("TinTucMoiNhat", "TinTuc")
                <div class="clearfix"></div>
                <div style="margin-top: 2%">
                    @Html.Action("TinTucXemNhieu2", "TinTuc")
                </div>
                <div class="clearfix"></div>
                <div style="margin-top: 2%">
                    <h1 style="text-align: center; color: green; font-weight: bold">Tin tức mới nhất</h1>
                    @Html.Action("TinTuc", "TinTuc")
                </div>
                <div class="clearfix"></div>
                <div style="margin-top: 2%">
                    <h1 style="text-align: center; color: green; font-weight: bold">Thủ thuật mới nhất</h1>
                    @Html.Action("ThuThuat", "TinTuc")
                </div>
                <div class="clearfix"></div>
                <div style="margin-top: 2%">
                    <h1 style="text-align: center; color: green; font-weight: bold">Kiến thức mới nhất</h1>
                    @Html.Action("KienThuc", "TinTuc")
                </div>
                <div class="clearfix"></div>

            </div>
            @Html.Action("TrangChu2", "HienThi")
            <div class="top_grid wow fadeInRight" data-wow-delay="0.4s">
                @Html.Action("TatCaCodeWebsite", "LocCode")
                <div class="clearfix"> </div>
                @Html.Action("TatCaCodeGame", "LocCode")
                <div class="clearfix"> </div>
                @Html.Action("TatCaCodeUngDung", "LocCode")
                <div class="clearfix"> </div>
            </div>
        </div>
        @Html.Action("ChanTK", "HienThi")
    </div>
</div>


